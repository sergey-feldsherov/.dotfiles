[general]
use-default-font=true
use-scrollbar=false
use-popup=false
start-fullscreen=true
fullscreen-hide-tabbar=true
set-window-title=true
compat-delete='delete-sequence'
display-n=-1
window-height=26
quick-open-command-line='gedit %(file_path)s'
gtk-prefer-dark-theme=true
window-valignment=0
display-tab-names=0
infinite-history=true
save-tabs-when-changed=false
use-trayicon=false
mouse-display=false
window-losefocus=true
window-halignment=0
prompt-on-quit=true
history-size=1000
start-at-login=true
use-vte-titles=true
scroll-keystroke=true
window-width=100
open-tab-cwd=true
window-tabbar=false
window-refocus=true
restore-tabs-startup=false
window-ontop=false
restore-tabs-notify=false
max-tab-name-length=100

[style]
cursor-shape=1

[style/background]
transparency=65

[style/font]
